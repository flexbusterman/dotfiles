#!/usr/bin/env bash
vmname=$(ls ~/VirtualBox\ VMs/ | wmenu -f "BigBlueTerm437 Nerd Font Mono 9" -n "#FFFFFF" -p "Start VM")
VBoxManage startvm "$vmname"

