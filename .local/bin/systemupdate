#!/bin/bash
eval "$(ssh-agent -s)"
ssh-add ~/.ssh/git
ssh-add ~/.ssh/digitalocean
pass git pull
dot pull
cd ~/.local/src/dwm/ && git pull && make && sudo make clean install
cd ~/.local/src/dwmblocks/ && git pull && make && sudo make clean install
cd ~/.local/src/st/ && git pull && make && sudo make clean install
yay -Syu
killall dwm
