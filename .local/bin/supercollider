# alacritty -t SuperCollider -e zsh -c "cd ~/Dropbox/SUPERCOLLIDER/; nvim -c \"set filetype=supercollider | SCNvimStart\"" &
# reaper &
# alacritty -t aug -e zsh -c "sleep 5; aconnect 128:7 16:0"

