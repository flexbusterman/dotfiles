{
  "vetur.ignoreProjectWarning": true,
  "prettier.semi": false,
  "prettier.endOfLine": "auto",
  "suggest.noselect": false,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "json",
    "shell"
  ],
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml",
        "categorize.cfg"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "svelte.enable-ts-plugin": true,
  "snippets.ultisnips.pythonPrompt": false
}
