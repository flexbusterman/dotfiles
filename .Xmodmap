! Clear Mods
clear Mod1
clear Mod4
clear Mod3

! Define before modifying
keycode 64 = Alt_L
keycode 108 = Multi_key
keycode 133 = Super_L

! Modifiers
add Mod1 = Alt_L
add Mod4 = Super_L

! Additional mappings
keycode 66 = Mode_switch

keysym h = h H Left
keysym l = l L Right
keysym k = k K Up
keysym j = j J Down
keysym p = p P Prior
keysym i = i I Home
keysym o = o O End
keysym n = n N Next
